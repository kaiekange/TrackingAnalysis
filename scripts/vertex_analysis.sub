# mkfile.sub -- submit job to run prepare simplified tuples

datatype = ZeroBias_noBS

executable = vertex_analysis.sh
arguments = $(datatype)

log = logs/condor/$(datatype)_vertex_analysis.log

request_cpus = 4
request_memory = 8G
request_disk = 10M

max_retries = 5

should_transfer_files = NO

queue 1
