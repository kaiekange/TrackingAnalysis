# ip_res_analysis.sub -- submit job to run prepare simplified tuples

datatype = ZeroBias

executable = ip_res_analysis.sh
arguments = $(Process) $(datatype)

log = logs/condor/$(datatype)_ip_res_analysis.log

request_cpus = 4
